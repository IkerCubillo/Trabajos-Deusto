package redSocial;

public class Publicacion {
	private String usuario = "";
	private int fechaPublicacion = 0;
	private int likes = 0;
	private String tipo = "";
	private Comentario[] comentarios = new Comentario[100];
	
	public Publicacion() {
	}
	
	public Publicacion(String usuario, int fechaPublicacion, int likes, String tipo) {
		this.usuario = usuario;
		this.fechaPublicacion = fechaPublicacion;
		this.likes = likes;
		this.tipo = tipo;
	}
	
	
	

}
