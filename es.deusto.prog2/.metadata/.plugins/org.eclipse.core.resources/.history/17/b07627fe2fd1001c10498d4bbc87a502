package carreraCoches;

public class Coche implements Runnable {	
	private String nombre = "";
	private int count = 0;
	
	public Coche(String nombre) {
		this.nombre = nombre;
	}
	
	@Override
	public void run() {
		for (int i = 0; i < 1000000000; i++) {
			count ++;
		}
		System.out.println("Coche " + nombre + " terminado");
	}
	
}