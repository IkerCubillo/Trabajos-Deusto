package redSocial;

public class Publicacion {
	private int fechaPublicacion = 0;
	private int likes = 0;
	private String tipo = "";
	private Comentario[] comentarios;
	
	public Publicacion() {
	}
	
	public Publicacion(int fechaPublicacion, int likes, String tipo, Comentario[] comentarios) {
		this.fechaPublicacion = fechaPublicacion;
		this.likes = likes;
		this.tipo = tipo;
		this.comentarios = comentarios;
	}
	
	

}
